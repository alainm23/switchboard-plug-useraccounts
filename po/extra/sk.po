#: data/useraccounts.policy.in.in:10
msgid "Manage user accounts"
msgstr "Spravujte používateľské účty"

#: data/useraccounts.policy.in.in:11
msgid "Authentication is required to change user data"
msgstr "Pre zmenu používateľských údajov sa vyžaduje overenie"

#: data/useraccounts.appdata.xml.in:5
msgid "User Accounts Settings"
msgstr "Nastavenia používateľských účtov"

#: data/useraccounts.appdata.xml.in:6
msgid ""
"Manage account permissions and configure user names, passwords, and photos"
msgstr ""
"Spravujte oprávnenia účtov a nastavujte používateľské mená, heslá a ich fotky"

#: data/useraccounts.appdata.xml.in:12
msgid "Minor updates:"
msgstr "Menšie aktualizácie:"

#: data/useraccounts.appdata.xml.in:14
msgid "New colorful avatar fallback"
msgstr "Nová náhradná farebná ikonka používateľa"

#: data/useraccounts.appdata.xml.in:15
msgid "Performance improvements"
msgstr "Zlepšenia výkonu"

#: data/useraccounts.appdata.xml.in:16
msgid "Improved tooltips and labels"
msgstr "Vylepšené popisy a štítky"

#: data/useraccounts.appdata.xml.in:17
msgid "Fix possible crash when deleting an account"
msgstr "Opravený možný pád pri odstraňovaní účtu"

#: data/useraccounts.appdata.xml.in:18 data/useraccounts.appdata.xml.in:25
#: data/useraccounts.appdata.xml.in:35 data/useraccounts.appdata.xml.in:45
msgid "Updated translations"
msgstr "Aktualizované preklady"

#: data/useraccounts.appdata.xml.in:32
msgid "Show more accurate reasons for locked settings"
msgstr "Zobrazovať presnejšie dôvody pre uzamknuté nastavenia"

#: data/useraccounts.appdata.xml.in:33
msgid "Improve reliability of account creation"
msgstr "Zlepšená spoľahlivosť vytvárania účtov"

#: data/useraccounts.appdata.xml.in:34
msgid "Get permission on demand for enabling or disabling accounts"
msgstr "Získavajte oprávnenia na požiadanie pre povolenie či zakázanie účtov"

#: data/useraccounts.appdata.xml.in:42
msgid "Show in sidebar if an account is disabled"
msgstr "V bočnom paneli zobrazovať, či je účet zakázaný"

#: data/useraccounts.appdata.xml.in:43
msgid "Ask for permission when adding and removing accounts"
msgstr "Spýtať sa na oprávnenie pri pridávaní a odstraňovaní účtov"

#: data/useraccounts.appdata.xml.in:44
msgid "Animate Infobars"
msgstr "Animovať informačné pruhy"
